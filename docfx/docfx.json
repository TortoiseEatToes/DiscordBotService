{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/DiscordBotService.csproj"
          ]
        }
      ],
      "dest": "artifacts/projects",
      "namespaceLayout": "flattened",
      "enumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [ "**/*.{md,yml}" ],
    "output": "artifacts/site",
    "template": [
      "default",
      "modern",
      "templates/discord"
    ],
    "globalMetadata": {
      "_appName": "DiscordBotService",
      "_appTitle": "DiscordBotService",
      "_appFooter": "Source <a href=\"https://github.com/TortoiseEatToes/DiscordBotService\">DiscordBotService</a>",
      "_enableSearch": true
    }
  }
}