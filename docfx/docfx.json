{
  "metadata": [
    {
      "src": [
        {
          "src": "../src/DiscordBotCommands",
          "files": [
            "DiscordBotCommands.csproj"
          ]
        }
      ],
      "dest": "artifacts/code/commands"
    },
    {
      "src": [
        {
          "src": "../src/DiscordBotService",
          "files": [
            "DiscordBotService.csproj"
          ]
        }
      ],
      "dest": "artifacts/code/service"
    },
    {
      "src": [
        {
          "src": "../src/SharedUtilities",
          "files": [
            "SharedUtilities.csproj"
          ]
        }
      ],
      "dest": "artifacts/code/shared"
    }
  ],
  "build": {
    "content": [ "**/*.{md,yml}" ],
    "output": "artifacts/site",
    "template": [
      "default",
      "modern",
      "templates/discord"
    ],
    "globalMetadata": {
      "_appName": "DiscordBotService",
      "_appTitle": "DiscordBotService",
      "_appFooter": "Source <a href=\"https://github.com/TortoiseEatToes/DiscordBotService\">DiscordBotService</a>",
      "_enableSearch": true
    }
  }
}